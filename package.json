{
  "name": "eplusout-dashboard",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "format": "prettier --write index.html",
    "tailwind": "tailwindcss -c tailwind.config.js -i ./src.css -o ./dist/tailwind.css --minify",
    "dev:css": "tailwindcss -c tailwind.config.js -i ./src.css -o ./dist/tailwind.css --watch",
    "build": "npm run tailwind"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.10",
    "postcss": "^8.4.41",
    "autoprefixer": "^10.4.20"
  }
}
