{
  "name": "eplusout-dashboard",
  "private": true,
  "version": "0.4.0",
  "scripts": {
    "dev": "npm run version:inject && npm run sample:generate && vite",
    "dev:host": "npm run version:inject && npm run sample:generate && vite --host",
    "version:inject": "node scripts/gen-version.cjs",
    "sample:generate": "node scripts/gen-sample-db.cjs",
    "build": "npm run version:inject && npm run sample:generate && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "test": "npm run test:unit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "ci": "npm run lint && npm run typecheck && npm run test && npm run build",
    "prepare": "husky"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.56.1",
    "@types/node": "^22.18.6",
    "@types/sql.js": "^1.4.9",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "autoprefixer": "^10.4.24",
    "eslint": "^9.39.1",
    "eslint-plugin-no-unsanitized": "^4.1.4",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.2.6",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "tailwindcss": "^3.4.19",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1",
    "vite": "^7.1.11",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@tailwindplus/elements": "^1.0.22",
    "d3": "^7.9.0",
    "sql.js": "^1.14.0"
  },
  "type": "module",
  "engines": {
    "node": ">=20"
  }
}
